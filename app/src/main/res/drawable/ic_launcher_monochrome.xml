    -->
    <path
        android:fillColor="#FFFFFFFF"
        android:pathData="M33.5,20C28.81,20 25,23.81 25,28.5v51C25,83.19 28.81,87 33.5,87h41C78.69,87 82.5,83.19 82.5,79.5v-51C82.5,23.81 78.69,20 74,20H33.5zM58,25.5h-8v-5.5h8V25.5z" />

    <!-- Transparent cross punch-through so the final tinted icon stays readable -->
    <path
        android:fillColor="#00000000"
        android:pathData="M39,51 L49,51 L49,41 L59,41 L59,51 L69,51 L69,61 L59,61 L59,71 L49,71 L49,61 L39,61 Z" />
